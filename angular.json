{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-alert": {
      "root": "projects/ng-alert",
      "sourceRoot": "projects/ng-alert/src",
      "projectType": "library",
      "prefix": "ng-alert",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ng-alert/tsconfig.lib.json",
            "project": "projects/ng-alert/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-alert/src/test.ts",
            "tsConfig": "projects/ng-alert/tsconfig.spec.json",
            "karmaConfig": "projects/ng-alert/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-alert/tsconfig.lib.json",
              "projects/ng-alert/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-alert-showcase": {
      "root": "projects/ng-alert-showcase/",
      "sourceRoot": "projects/ng-alert-showcase/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-alert-showcase",
            "index": "projects/ng-alert-showcase/src/index.html",
            "main": "projects/ng-alert-showcase/src/main.ts",
            "polyfills": "projects/ng-alert-showcase/src/polyfills.ts",
            "tsConfig": "projects/ng-alert-showcase/tsconfig.app.json",
            "assets": [
              "projects/ng-alert-showcase/src/favicon.ico",
              "projects/ng-alert-showcase/src/assets"
            ],
            "styles": [
              "projects/ng-alert-showcase/src/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ng-alert-showcase/src/environments/environment.ts",
                  "with": "projects/ng-alert-showcase/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-alert-showcase:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-alert-showcase:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-alert-showcase:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-alert-showcase/src/test.ts",
            "polyfills": "projects/ng-alert-showcase/src/polyfills.ts",
            "tsConfig": "projects/ng-alert-showcase/tsconfig.spec.json",
            "karmaConfig": "projects/ng-alert-showcase/karma.conf.js",
            "styles": [
              "projects/ng-alert-showcase/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/ng-alert-showcase/src/favicon.ico",
              "projects/ng-alert-showcase/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-alert-showcase/tsconfig.app.json",
              "projects/ng-alert-showcase/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-alert-showcase-e2e": {
      "root": "projects/ng-alert-showcase-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ng-alert-showcase-e2e/protractor.conf.js",
            "devServerTarget": "ng-alert-showcase:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-alert-showcase:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/ng-alert-showcase-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-alert"
}